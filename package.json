{
  "name": "survey-system",
  "version": "1.0.0",
  "description": "Survey system inspired in Google forms",
  "main": "index.js",
  "scripts": {
    "lint": "tslint --project '.'",
    "compile": "tsc",
    "build": "npm run compile && npx webpack -p",
    "start": "node dist/server/index.js",
    "start:dev": "nodemon --config \"./nodemon.json\"/",
    "start:test": "NODE_ENV=test PORT=4444 nodemon --config \"./nodemon.json\"/",
    "db:seed": "knex seed:run",
    "db:seed:test": "NODE_ENV=test knex seed:run",
    "db:reset": "npx ts-node db/reset.ts",
    "db:reset:test": "NODE_ENV=test npx ts-node db/reset.ts",
    "db:migrate": "knex migrate:latest",
    "db:migrate:test": "NODE_ENV=test knex migrate:latest",
    "cy:open": "cypress open",
    "cy:run": "cypress run --config video=false",
    "test:ci": "start-server-and-test start:test http-get://localhost:4444 cy:run"
  },
  "keywords": [
    "nodejs",
    "react",
    "survey",
    "forms"
  ],
  "author": "Ricardo Rojas",
  "license": "ISC",
  "dependencies": {
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.0.26",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "moment": "^2.27.0",
    "objection": "^2.2.1",
    "pg": "^8.2.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@cypress/webpack-preprocessor": "^5.4.5",
    "@types/express": "^4.17.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.157",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "autoprefixer": "^9.8.4",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.6.0",
    "cypress": "^5.0.0",
    "dotenv": "^8.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "flexboxgrid": "^6.3.1",
    "html-webpack-plugin": "^4.3.0",
    "immer": "^7.0.7",
    "lodash": "^4.17.19",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "postcss-loader": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "sass-loader": "^9.0.2",
    "source-map-loader": "^1.0.1",
    "start-server-and-test": "^1.11.3",
    "style-loader": "^1.2.1",
    "tailwindcss": "^1.4.6",
    "ts-lint": "^4.5.1",
    "ts-loader": "^8.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2"
  }
}
